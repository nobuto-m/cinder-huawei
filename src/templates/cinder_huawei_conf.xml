<config>
    <Storage>
        <Product>{{ options.product }}</Product>
        <UserName>{{ options.username }}</UserName>
        <UserPassword>{{ options.password }}</UserPassword>
        <RestURL>{{ options.rest_url }}</RestURL>
    </Storage>
    <LUN>
        <StoragePool>{{ options.storage_pool }}</StoragePool>
    </LUN>
</config>
