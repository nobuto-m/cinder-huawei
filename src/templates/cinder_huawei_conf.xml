<?xml version='1.0' encoding='UTF-8'?>
<config>
    <Storage>
        <Product>{{ options.product }}</Product>
        <UserName>!$$${{ options.username_base64 }}</UserName>
        <UserPassword>!$$${{ options.password_base64 }}</UserPassword>
        <RestURL>{{ options.rest_url }}</RestURL>
    </Storage>
    <LUN>
        <StoragePool>{{ options.storage_pool }}</StoragePool>
    </LUN>
</config>
